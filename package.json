{
  "name": "steal-less",
  "version": "1.3.1",
  "description": "less plugin for StealJS",
  "main": "less.js",
  "scripts": {
    "document": "bit-docs",
    "test": "testee test/test.html test/unit.html --browsers firefox --reporter Spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/steal-less.git"
  },
  "keywords": [
    "StealJS",
    "LESS"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stealjs/steal-less/issues"
  },
  "homepage": "https://github.com/stealjs/steal-less",
  "dependencies": {
    "less": "2.4.0 - 2.5.3",
    "steal-css": "^1.2.0"
  },
  "devDependencies": {
    "bit-docs": "0.0.9",
    "bower": "^1.4.1",
    "qunitjs": "^1.23.1",
    "steal": "^1.0.0",
    "steal-qunit": "^1.0.0",
    "testee": "^0.3.0"
  },
  "steal": {
    "plugins": [
      "steal-css"
    ],
    "envs": {
      "build": {
        "map": {
          "steal-less/less-engine": "steal-less/less-engine-node"
        }
      },
      "server-development": {
        "map": {
          "steal-less/less-engine": "steal-less/less-engine-node"
        }
      },
      "server-production": {
        "map": {
          "steal-less/less-engine": "steal-less/less-engine-node"
        }
      },
      "bundle-build": {
        "map": {
          "steal-less/less-engine": "steal-less/less-engine-node"
        },
        "meta": {
          "steal-less/less": {
            "useLocalDeps": true
          }
        }
      }
    },
    "ext": {
      "less": "steal-less"
    }
  },
  "bit-docs": {
    "dependencies": {
      "bit-docs-glob-finder": "^0.0.5",
      "bit-docs-dev": "^0.0.3",
      "bit-docs-js": "^0.0.3",
      "bit-docs-generate-readme": "^0.0.8"
    },
    "glob": {
      "pattern": "**/*.{js,md}",
      "ignore": "node_modules/**/*"
    },
    "readme": {
      "apis": [
        "steal-less"
      ]
    },
    "parent": "steal-less"
  }
}
